<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yiyan.boot3.dao.IUserDao">

    <select id="selectByUsername" resultType="com.yiyan.boot3.model.po.UserPO">
        select *
        from user u
        where u.username = #{username}
    </select>
</mapper>
